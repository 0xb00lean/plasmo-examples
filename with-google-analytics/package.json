{
  "name": "with-google-analytics",
  "displayName": "With google analytics",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "lgvic",
  "packageManager": "pnpm@7.1.2",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/chrome": "0.0.186",
    "@types/node": "17.0.34",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "plasmo": "latest",
    "prettier": "2.6.2",
    "typescript": "4.6.4"
  },
  "manifest": {
    "host_permissions": [
      "https://ssl.google-analytics.com",
      "https://*/*"
    ]
  }
}
