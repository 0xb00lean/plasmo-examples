{
  "name": "with-storage",
  "displayName": "With storage",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "contributors": [
    "coldsauce"
  ],
  "scripts": {
    "dev": "plasmo dev",
