{
  "name": "with-inbox-sdk",
  "displayName": "With inbox sdk",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@inboxsdk/core": "1.2.5",
    "plasmo": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@types/chrome": "0.0.243",
    "@types/inboxsdk": "2.0.10",
    "@types/node": "20.5.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "kefir": "3.8.8",
    "prettier": "3.0.2",
    "typescript": "5.1.6"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "scripting"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "@inboxsdk/core/pageWorld.js"
        ],
        "matches": [
          "https://mail.google.com/*",
          "https://inbox.google.com/*"
        ]
      }
    ]
  },
  "contributors": [
    "lgvic",
    "yashwadhia"
  ]
}
